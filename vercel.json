{
  "cleanUrls": true,
  "rewrites": [
    { "source": "/ceremony", "destination": "/ceremony/index.html" },
    { "source": "/ceremony/", "destination": "/ceremony/index.html" },
    { "source": "/ceremony/(.*)", "destination": "/ceremony/$1" }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        { "key": "X-Content-Type-Options", "value": "nosniff" }
      ]
    }
  ]
}
